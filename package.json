{
  "name": "nlcst-emoticon-modifier",
  "version": "1.0.0",
  "description": "Emoticons in NLCST",
  "license": "MIT",
  "keywords": [
    "nlcst",
    "emoticons"
  ],
  "dependencies": {
    "nlcst-to-string": "^2.0.0",
    "unist-util-modify-children": "^1.0.0"
  },
  "repository": "wooorm/nlcst-emoticon-modifier",
  "author": "Titus Wormer <tituswormer@gmail.com>",
  "files": [
    "data/emoticon.json",
    "index.js"
  ],
  "devDependencies": {
    "array-uniq": "^1.0.3",
    "browserify": "^13.0.0",
    "emoticon": "^2.0.0",
    "esmangle": "^1.0.0",
    "istanbul": "^0.4.0",
    "mdast": "^1.0.0",
    "mdast-comment-config": "^1.0.0",
    "mdast-github": "^1.0.0",
    "mdast-lint": "^1.0.0",
    "mdast-slug": "^1.0.0",
    "mdast-validate-links": "^1.0.0",
    "mocha": "^3.0.0",
    "retext": "^4.0.0",
    "retext-english": "^2.0.0",
    "xo": "^0.17.1"
  },
  "scripts": {
    "test-api": "mocha --check-leaks test/index.js",
    "test-coverage": "istanbul cover _mocha -- test/index.js",
    "test-travis": "npm run test-coverage",
    "test": "npm run test-api",
    "lint": "xo",
    "make": "npm run lint && npm run test-coverage",
    "bundle": "browserify index.js --no-builtins -s nlcstEmoticonModifier > nlcst-emoticon-modifier.js",
    "postbundle": "esmangle nlcst-emoticon-modifier.js > nlcst-emoticon-modifier.min.js",
    "build-data": "node script/build-data.js",
    "build-md": "mdast . --quiet",
    "build": "npm run build-data && npm run bundle && npm run build-md"
  },
  "xo": {
    "space": true,
    "rules": {
      "guard-for-in": "off"
    },
    "ignore": [
      "nlcst-emoticon-modifier.js"
    ]
  }
}
